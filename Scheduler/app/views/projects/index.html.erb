<p id="notice"><%= notice %></p>

<h1>Projects</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Data received</th>
      <th>Date due</th>
      <th>Words to be read</th>
      <th>Words to be typed</th>
      <th>Users words read per min</th>
      <th>Users words typed per min</th>
      <th>Total estimate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.title %></td>
        <td><%= project.data_received %></td>
        <td><%= project.date_due %></td>
        <td><%= project.words_to_be_read %></td>
        <td><%= project.words_to_be_typed %></td>
        <td><%= project.users_words_read_per_min %></td>
        <td><%= project.users_words_typed_per_min %></td>
        <td><%= project.total_estimate %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project', new_project_path %>
